{
    "extends": "airbnb",
    "rules": {
        "arrow-body-style": 0,
        "no-param-reassign": 0,
        "react/jsx-filename-extension": 0,
        "indent": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "object-shorthand": 0
